<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE configuration PUBLIC "-//mybatis.org//DTD Config 3.0//EN" "mybatis-3-config.dtd" >
<configuration>

	<settings>
		<setting name="jdbcTypeForNull" value="NULL"/>
	</settings>
	
	<typeAliases>
		<typeAlias type="com.kh.dh.member.model.vo.Member" alias="Member"/>
		<typeAlias type="com.kh.dh.board.model.vo.Board" alias="Board"/>
		<typeAlias type="com.kh.dh.board.model.vo.CodeChunk" alias="CodeChunk"/>
		<typeAlias type="com.kh.dh.code.model.vo.Room" alias="Room" />
		<typeAlias type="com.kh.dh.code.model.vo.RoomDTO" alias="RoomDTO" />
	</typeAliases>
	
	 <mappers>
	 	<mapper resource="/mappers/member-mapper.xml" />
	 	<mapper resource="/mappers/board-mapper.xml" />
